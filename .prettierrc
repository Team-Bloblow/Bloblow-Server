{
  "trailingComma": "es5",
  "semi": true,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "printWidth": 100,
  "arrowParens": "always",
  "endOfLine": "auto",
  "bracketSpacing": true,
  "importOrder": ["^react", "<THIRD_PARTY_MODULES>", "", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
